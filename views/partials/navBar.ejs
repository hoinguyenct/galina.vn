                <li><a href="#about"><%= translation.Introduce %></a></li>
                <li><a href="#gallery"><%= translation.Gallery %></a></li>
                <li><a href="#wedding"><%= translation.wedding %></a></li>
                <li><a href="#conference"><%= translation.conference %></a></li>
                <li><a href="#contact"><%= translation.Contact_us %></a></li>      
            <% } else if (pageID == 4) {%> <!-- show -->
                <li><a href="#home"><%= translation.Home %></a></li>
                <li><a href="#about"><%= translation.Introduce %></a></li>
                <li><a href="#rooms"><%= translation.show %></a></li>
                <li><a href="#gallery"><%= translation.Gallery %></a></li>
                <li><a href="#who-we-are"><%= translation.AboutUs %></a></li>
                <li><a href="#contact"><%= translation.Contact_us %></a></li>      
            <% } else if (pageID == 5) {%> <!-- lake view -->
                <li><a href="#home"><%= translation.Home %></a></li>
                <li><a href="#about"><%= translation.Introduce %></a></li>
                <li><a href="#rooms"><%= translation.Accomodation %></a></li>
