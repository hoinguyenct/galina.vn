                <li><a href="#rooms"><%= translation.Accomodation %></a></li>
            <%}%>

          
      </ul>
      <div class="selector"></div>
  </nav>
  
  <!-- Module - Objects -->
  <div class="module module-object" data-toggle="panel-objects">
      <img class="object-image" src="img/photos/object_thumbnail.png" alt="" style="height: 36px">
      <span class="object-name"><%= title %></span>
      <span class="object-indicator"><i class="ti ti-menu"></i></span>
  </div>
  
  <!-- Module - Language -->
  <nav class="module module-language mr-3">
    <ul class="nav nav-main-horizontal nav-language">
        <li><a id="lang-vi" class="lang lang-vi" data='.lang-vi' href="/<%= ref %>?clang=vi">VN</a></li>
        <li><a id="lang-en" class="lang lang-en" data='.lang-en' href="/<%= ref %>?clang=en">EN</a></li>
        <li><a id="lang-rus"class="lang lang-rus" data='.lang-rus' href="/<%= ref %>?clang=rus">RUS</a></li>
        <li><a id="lang-kr" class="lang lang-kr" data='.lang-kr' href="/<%= ref %>?clang=kr">KR</a></li>
    </ul>
  </nav>

  <!-- Module - Navigation Toggle -->
  <div class="module module-nav-toggle">
    <a href="#" id="nav-toggle" class="nav-toggle">
        <span></span><span></span><span></span><span></span>
    </a>
